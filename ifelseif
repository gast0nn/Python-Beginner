x = 1
if x == 1:
    print(" x = 1 ")

x = 2
if x > 1:
    print("x is greater than 1")
else:
    print("x is not greater than 1")

hour = 31
if hour < 15:
    print("good morning")
elif hour < 20:
    print("good afternoon")
else:
    print("goood night")

grade = 89
if grade > 90:
    print("A")




