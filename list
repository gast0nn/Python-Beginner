my_list = [1, 2, 3, 4, 5]
print(my_list[1])
people_list = ["Gaston", "Lari", "Leo"]
print(people_list[0:2]) #EXCEPECIFIC THINGS FROM YOUR LIST

my_list = [1, 2, 3, 4, 5]
print(my_list)
my_list.append (1000) #APPEND ONE THING TO LIST
print(my_list)
my_list.remove (5) #REMOVE THINGS FROM LIST
print(my_list)
my_list.insert (2, 1000) #INSERT THINGS TO LIST
print(my_list)
my_list.pop(0) #POP THINGS TO LIST
print(my_list)
my_list.sort() #SORT HIGHER TO LOWER YOUR LIST
print(my_list)
